#include <stdio.h>
#include "system.h"
#include <alt_types.h>
#include "altera_avalon_pio_regs.h"
#include "sys/alt_irq.h"
#include "KEY.h"
#include "io.h"
// X, Y, A, B, TR, TL, START, SELECT,


int main() {
	printf("Teste de Botao\n");
	initialize_joystick();
	//set_KEY_callback(as0);
	//set_KET_interrput(255, 0);

	while (1){
		for (int i = 0; i < 8; i++){
			if (edge_KEY(i,0)){
				printf("Botao precionado %d\n",i);
			}
		}

	}

	 return 0;
}

