#include "system.h"
#include "sys/alt_irq.h"
#include "KEY.h"
// X, Y, A, B, TR, TL, START, SELECT,


int main() {
	printf("===============================================\n");
	printf("Teste de detecçao de pressionamento de botao\n");
	initialize_joystick();
	//set_KEY_callback(as0);
	//set_KET_interrput(255, 0);

	while (1){
		for (int i = 0; i < 8; i++){
			if (edge_KEY(i,0)){
				printf("Botao precionado %d\n",i);
			}
		}

	}

	 return 0;
}

