#include <stdio.h>
#include "system.h"
#include <alt_types.h>
#include "altera_avalon_pio_regs.h"
#include "sys/alt_irq.h"
#include "KEY.h"
#include "io.h"
// X, Y, A, B, TR, TL, START, SELECT,

int a = 0;

// X, Y, A, B, TR, TL, START, SELECT,
void as0() {
	if (edge_KEY(0,0) == 1){
		a = 0;
	} else if (edge_KEY(1,0) == 1){
		a = 0;
	} else if (edge_KEY(2,0) == 1){
		a = 0;
	} else if (edge_KEY(3,0) == 1){
		a = 0;
	} else if (edge_KEY(4,0) == 1){
		a = 0;
	} else if (edge_KEY(5,0) == 1){
		a = 0;
	} else if (edge_KEY(6,0) == 1){
		a = 0;
	} else if (edge_KEY(7,0) == 1){
		a = 0;
	}

}

int main() {
	printf("Teste de Botao\n");
	initialize_joystick();
	set_KEY_callback(as0);
	set_KET_interrput(255, 0);

	while (1){
	}

	 return 0;
}

