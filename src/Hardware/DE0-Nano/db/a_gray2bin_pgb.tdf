--synthesis_resources = 
SUBDESIGN a_gray2bin_pgb
( 
	bin[3..0]	:	output;
	gray[3..0]	:	input;
) 
VARIABLE 
	xor0	: WIRE;
	xor1	: WIRE;
	xor2	: WIRE;

BEGIN 
	bin[] = ( gray[3..3], xor2, xor1, xor0);
	xor0 = (gray[0..0] $ xor1);
	xor1 = (gray[1..1] $ xor2);
	xor2 = (gray[3..3] $ gray[2..2]);
END;
