module main (
    input  wire                     clk, rst_n ,we,
    input  wire  [7:0]  buttons,
    output [7:0] leds
);
    
    top_button_controller tbc (
        /*I*/ .clk(clk),
        /*I*/ .rst_n(rst_n),
        /*I*/ .we(we),
        /*I*/ .buttons(buttons),
        /*I*/ .register_addr(0),
        /*I*/ .wr_data(8'b0),
        /*O*/ .rd_data(reg_a)
    );

    assign leds = reg_a[7:0];
endmodule